<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tl.blog.admin.mapper.SysUserMapper">
    <select id="userlLogin" parameterType="org.tl.blog.admin.entity.SysUser" resultType="org.tl.blog.admin.entity.SysUser">
      select * from sys_user where username=#{username} and passoword=#{passoword}
    </select>
</mapper>